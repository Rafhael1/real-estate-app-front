{
  "name": "real-estate-listing-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "test": "jest",
    "test:watch": "jest -- --watch",
    "test:cy": "cypress open-ct",
    "test:e2e": "cypress open",
    "build": "tsc && vite build",
    "docker:build": "docker build -f Dockerfile.dev -t real-estate-app-image-dev .",
    "docker:start": "docker run --rm -it --name real-estate-app-container -p 3000:3000 -v $(pwd):/app real-estate-app-image-dev",
    "docker:compose": "docker-compose up",
    "tsc": "tsc",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/lab": "^5.0.0-alpha.80",
    "@mui/material": "5.7.0",
    "@mui/styled-engine-sc": "5.7.0",
    "@mui/styles": "^5.7.0",
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^0.27.2",
    "react": "18.1.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "18.1.0",
    "react-dropzone": "^14.2.2",
    "react-hook-form": "^7.31.3",
    "react-redux": "^8.0.1",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "styled-components": "^5.3.5",
    "typeface-quicksand": "^1.1.13",
    "typescript": "^4.6.4",
    "vite": "^3.0.8",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-flow": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@cypress/react": "^5.12.4",
    "@cypress/webpack-dev-server": "^1.8.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^18.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux": "^3.6.31",
    "@types/redux-form": "^8.3.5",
    "@types/redux-mock-store": "^1.0.3",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-react": "^2.0.1",
    "babel-jest": "28.1.0",
    "cypress": "^9.6.1",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "expect": "^28.0.2",
    "file-loader": "^6.2.0",
    "husky": "^7.0.4",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "react-error-overlay": "6.0.11",
    "redux-devtools-extension": "^2.13.9",
    "redux-mock-store": "^1.5.4",
    "ts-node": "^10.7.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-env-compatible": "^1.1.1",
    "vite-plugin-svgr": "^2.2.1",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/*"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "yarn lint --max-warnings=0",
      "jest --bail --findRelatedTests"
    ]
  }
}
